%h1 Please feel your personal data to continue

= form_for @dispatcher, :url => user_path do |f|
  = f.label :email, 'Email'
  = f.text_field :email, :disabled => true
  = f.label :first_name, 'First Name'
  = f.text_field :first_name
  = error_tag @dispatcher.errors[:first_name]
  = f.label :last_name, 'Last Name'
  = f.text_field :last_name
  = error_tag @dispatcher.errors[:last_name]
  = f.label :institution_id, 'Institution'
  = f.select :institution_id, ''
  = f.label :password, 'Password'
  = f.password_field :password
  = error_tag @dispatcher.errors[:password]
  = f.label :password_confirmation, 'Password Confirmation'
  = f.password_field :password_confirmation
  = f.submit :OK
