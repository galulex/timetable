%td{ :colspan => 3 }
  = form_for [@schedule, @object], :remote => true do |f|
    %table
      %tr
        %td.start_at= f.time_select :start_at, :minute_step => 5, :class => :time_select
        %td.finish_at= f.time_select :finish_at, :minute_step => 5, :class => :time_select
        %td.actions= f.submit t("ok")
